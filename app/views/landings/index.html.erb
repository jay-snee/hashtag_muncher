<div class="row margin-bottom">
  <div class="col-sm-12">
    <h1>
      <%= current_user.twitter_screen_name %>
      <div class="pull-right">
        <%= simple_form_for Search.new(user: current_user), method: :post do |f| %>
          <%= f.submit "Munch my Hashtags", class: "btn btn-primary" %>
        <% end %>
      </div>
    </h1>
  </div>
</div>

<div class="row top-buffer">
  <div class="col-sm-8">
    <ul>
      <%= current_user.methods.each do |m| %>
        <li><%= m %></li>
      <% end %>
    </ul>




  </div>
  <div class="col-sm-4">

  </div>
</div>
